<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <LinearGradientBrush x:Key="Stoped" EndPoint="0.5,1" StartPoint="0.5,0">
        <GradientStop Color="#FFC34646"/>
        <GradientStop Color="#FFE61B1B" Offset="0.527"/>
        <GradientStop Color="#FFC34646" Offset="0.99"/>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="Started" EndPoint="0.5,1" StartPoint="0.5,0">
        <GradientStop Color="#FF56C346"/>
        <GradientStop Color="#FF34E61B" Offset="0.527"/>
        <GradientStop Color="#FF56C346" Offset="0.99"/>
    </LinearGradientBrush>

    <SolidColorBrush x:Key="logError" Color="Red"/>
    <SolidColorBrush x:Key="logInfo" Color="#FF2BDAFB"/>
    <SolidColorBrush x:Key="logSuccess" Color="#FF0CA91D"/>
</ResourceDictionary>